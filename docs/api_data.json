rence",    "title": "Next Occurence",    "name": "_Next_Occurrence_",    "description": "<p>This endpoint returns the next occurrence(s) of a specified day type or period (not including the current occurrence, if applicable).</p>",    "group": "Schedule",    "version": "1.0.0",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "allowedValues": [              "\"US\"",              "\"MS\""            ],            "optional": false,            "field": "school",            "defaultValue": "US",            "description": "<p>School calendar to use</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "maxResults",            "defaultValue": "1",            "description": "<p>the maximum number of future occurrences to be returned</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "date",            "description": "<p>an ISO 8061 date string to search forward from</p>"          }        ],        "Required": [          {            "group": "Required",            "type": "String",            "allowedValues": [              "\"day_type\"",              "\"period\""            ],            "optional": false,            "field": "type",            "description": "<p>A string specifying the type of requested resource</p>"          },          {            "group": "Required",            "type": "String",            "optional": false,            "field": "identifier",            "description": "<p>A string specifying the type of requested resource e.g &quot;A&quot;, &quot;Period 1&quot;, &quot;Advisory Office Hours&quot;</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "periods",            "description": "<p>List of periods if type is &quot;period&quot;</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "periods.title",            "description": "<p>Day Type</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "periods.start_time",            "description": "<p>Start time of period in UTC timezone</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "periods.end_time",            "description": "<p>End time of period in UTC timezone</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "periods.day",            "description": "<p>The period's associated day</p>"          },          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "days",            "description": "<p>List of days if type is &quot;day_type&quot;</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "days.date",            "description": "<p>Date in ISO8061 Format, UTC time</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "days.type",            "description": "<p>Letter Day</p>"          }        ]      },      "examples": [        {          "title": "Success-Example:",          "content": "[\n    {\n        \"day\": \"\"\n        \"start_time\" : \"\",\n        \"end_time\": \"\",\n        \"title\": \"Period 1\",\n 